{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "noEmit": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "target": "esnext",
    "allowJs": true,
    "baseUrl": "app/",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    }
  }
}